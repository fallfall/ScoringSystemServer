        <div class="content_checkbox_column">
          <label class="content_checkbox_label">
              <input name="conent_1" type="checkbox" value="产品知识比较陌生" />
              产品知识比较陌生
            </label>
          <label class="content_checkbox_label">
              <input name="Fruit" type="checkbox" value="没有介绍新品和促销装" />
              没有介绍新品和促销装
            </label>
        </div>
      </div>
      <!-- 一星 -->
      <div class="hide" id="skill_1">
