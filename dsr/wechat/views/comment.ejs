      <div class="content_score">
        <div class="content_title">
          <h4>助销服务</h4>
        </div>
        <div class="content_star" id="help">
          <span class="icon-star-empty icon" value-id="1"></span>
          <span class="icon-star-empty icon" value-id="2"></span>
          <span class="icon-star-empty icon" value-id="3"></span>
          <span class="icon-star-empty icon" value-id="4"></span>
