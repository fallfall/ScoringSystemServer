<body>
  <div class="page" id="page_comment">
    <div class="box">
      <div class="box_comment">
        <span class="box_comment_text">你好，<strong id="name"></strong></span>
      </div>
      <div class="box_score">
        <span class="box_score_text">积分：<strong id="score"></strong></span>
      </div>
    </div>

    <div class="info">
      <div class="weui_cells weui_cells_form">

        <div class="weui_cell">
          <div class="weui_cell_hd"><label class="weui_label">DSR姓名</label></div>
          <div class="weui_cell_bd weui_cell_primary">
            <input class="weui_input" id="dsrname" type="text" placeholder="请选择DSR的姓名">
          </div>
        </div>

        <div class="weui_cell">
          <div class="weui_cell_hd"><label for="" class="weui_label">到店日期</label></div>
          <div class="weui_cell_bd weui_cell_primary">
            <input class="weui_input" type="text" id="date" placeholder="请选择到店日期" />
          </div>
        </div>
      </div>
    </div>

